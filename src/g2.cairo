type Fq = u256;
type Fq2 = (Fq, Fq);

// Twisted BN curve
// E'/Fq2 : y^2 = x^3 + b/xi

#[derive(Copy, Drop)]
struct AffineG2 {
    x: Fq2,
    y: Fq2
}


fn pt(x1: Fq, x2: Fq, y1: Fq, y2: Fq) -> AffineG2 {
    AffineG2 { x: (x1, x2), y: (y1, y2) }
}


